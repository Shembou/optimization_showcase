# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type User {
  id: Int!
  name: String!
  language: String!
  bio: String!
  version: Int!
}

type Query {
  users: [User!]!
}

input NewUser {
  name: String!
  language: String!
  bio: String!
  version: Int!
}

type Mutation {
  createUser(input: NewUser!): User!
}
